{
   "manifest_version": 3,
   "name": "Pingfence",
   "version": "1.0.9",
   "description": "Pingfence: A new way to stay secure",
   "icons": {
      "128": "img0001-modified-modified.png"
   },
   "background": {
      "service_worker": "background.service.js"
   },
   "action": {
      "default_icon": "img0001-modified-modified.png",
      "default_title": "Pingfence",
      "default_popup": "popup.html"
   },
   "key": "",
   "permissions": [
      "tabs",
      "storage",
      "notifications",
      "webRequest",
      "webRequestBlocking",
      "downloads",
      "scripting"
   ],
   "host_permissions": [
      "<all_urls>"
   ],
   "update_url": "https://clients2.google.com/service/update2/crx"
}
